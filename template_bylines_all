												{spec_sec_title = Weekend.content[0].title}
												{spec_sec_img = Weekend.content[0].image}
												{post_url = Weekend.content[0].url}
												{post_dek = Weekend.content[0].vars.mj_dek}
												{post_byline = upper(Weekend.content[0].author)}
												{found_it = true}
												{/case}
												{/switch}

												{if !found_it}
												{foreach Everything.content as c}
												{foreach c.tags as tag}
												{if tag == tag1 || tag == tag2}
												{*section_name = 'Health and Environment'*}
												{spec_sec_title = c.title}
												{spec_sec_img = c.image}
												{spec_sec_img_credit = ''}
												{post_url = c.url}
												{post_dek = c.vars.mj_dek}
												{post_byline = upper(c.author)}
												{found_it = true}
												{break}
												{/if}
												{/foreach}
												{if found_it}
												{break}
												{/if}
												{/foreach}
												{/if}
												{/if}</div>
