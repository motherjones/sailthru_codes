											<div style="font-size:16px;text-align:center;font-family:Georgia, Times, Times New Roman, serif">{found_it = false}
												{section_name = ''}
												{spec_sec_title = ''}
												{spec_sec_img = ''}
												{spec_sec_img_credit = ''}
												{post_url = ''}
												{post_dek = ''}
												{post_byline = ''}
												{day_of_week = date('EEEE',time('now'))}
												{img_url = ''}

												{if Newsletter.content[0].vars.override_url != null}
												{spec_sec_title = Newsletter.content[0].vars.override_title}
												{spec_sec_img = Newsletter.content[0].vars.override_img}
												{spec_sec_img_credit = Newsletter.content[0].vars.override_img_credit}
												{post_url = Newsletter.content[0].vars.override_url}
												{post_dek = Newsletter.content[0].vars.override_dek}
												{post_byline = Newsletter.content[0].vars.override_byline}
												{else}
												{switch day_of_week}
												{case 'Monday'}
												{img_url = 'https://sailthru-media.s3.amazonaws.com/composer/images/sailthru-prod-6ho/label-health-environment_50.png'}
												{spec_sec_title = HealthEnvironment.content[0].title}
												{spec_sec_img = HealthEnvironment.content[0].image}
												{post_url = HealthEnvironment.content[0].url}
												{post_dek = HealthEnvironment.content[0].vars.mj_dek}
												{post_byline = upper(HealthEnvironment.content[0].author)}
												{found_it = true}
												{/case}
												{case 'Tuesday'}
												{img_url = 'https://sailthru-media.s3.amazonaws.com/composer/images/sailthru-prod-6ho/label-food_50.png'}
												{tag1 = 'food'}
												{tag2 = 'food'}
												{/case}
												{case 'Wednesday'}
												{img_url = 'https://sailthru-media.s3.amazonaws.com/composer/images/sailthru-prod-6hn/Daily%20Newsletter/Creative%20Assets/label-mojo-podcast_50.png'}
												{tag1 = 'the-mother-jones-podcast'}
												{tag2 = 'the-mother-jones-podcast'}
												{/case}
												{case 'Thursday'}
												{img_url = 'https://sailthru-media.s3.amazonaws.com/composer/images/sailthru-prod-6hn/Daily%20Newsletter/Creative%20Assets/label-big-feature_50.png'}
												{tag1 = 'the-big-feature'}
												{tag2 = 'the-big-feature'}
												{/case}
